<div id="monitoringAgentReportDownload" class="reportsDownload" xmlns:spring="http://www.springframework.org/tags"
     xmlns:util="urn:jsptagdir:/WEB-INF/tags/util">
    <spring:url value="/analysisData/monitoringAgentReport.xls" var="download_monitoring_agent_url"/>
    <util:panel id="monitoringAgentReportPane" title="MonitoringAgents Report" openPane="true">
        <form id="monitoringAgentsDownloadForm" action="${download_monitoring_agent_url}" method="GET">
            <div class="control-group">
                <input type="submit" id="downloadMonitoringAgentReport" value="DOWNLOAD AS EXCEL"/>
            </div>
        </form>
    </util:panel>
</div>